{
  "groups": [
    {
      "name": "snowman.prop",
      "type": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties"
    }
  ],
  "properties": [
    {
      "name": "snowman.prop.chunk",
      "type": "java.lang.Integer",
      "description": "每次获取ID数量,默认10",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": 10
    },
    {
      "name": "snowman.prop.group-id",
      "type": "java.lang.String",
      "description": "客户端组ID,必须项",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties"
    },
    {
      "name": "snowman.prop.host",
      "type": "java.lang.String",
      "description": "snowman服务地址,默认localhost",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": "localhost"
    },
    {
      "name": "snowman.prop.mode",
      "type": "java.lang.String",
      "description": "ID生成算法,包括数字、雪花算法、UUID",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": "normal"
    },
    {
      "name": "snowman.prop.name",
      "type": "java.lang.String",
      "description": "项目名称",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties"
    },
    {
      "name": "snowman.prop.path",
      "type": "java.lang.String",
      "description": "snowman请求路由",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": "\/"
    },
    {
      "name": "snowman.prop.port",
      "type": "java.lang.Integer",
      "description": "snowman服务端口,默认8080",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": 8080
    },
    {
      "name": "snowman.prop.protocol",
      "type": "java.lang.String",
      "description": "请求协议",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": "http"
    },
    {
      "name": "snowman.prop.server-id",
      "type": "java.lang.String",
      "description": "客户端ID,必须项",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties"
    },
    {
      "name": "snowman.prop.timeout",
      "type": "java.lang.Long",
      "description": "请求超时时间,单位毫秒,默认30秒",
      "sourceType": "cc.kevinlu.snow.autoconfigure.SnowmanProperties",
      "defaultValue": 30000
    }
  ],
  "hints": [
    {
      "name": "snowman.prop.mode",
      "values": [
        {
          "value": "normal",
          "description": "使用常规数字生成ID"
        },
        {
          "value": "snowflake",
          "description": "使用雪花算法生成ID"
        },
        {
          "value": "uuid",
          "description": "使用UUID算法生成ID"
        }
      ]
    },
    {
      "name": "snowman.prop.protocol",
      "values": [
        {
          "value": "http",
          "description": "HTTP"
        },
        {
          "value": "https",
          "description": "HTTPS"
        },
        {
          "value": "tcp",
          "description": "TCP"
        }
      ]
    }
  ]
}